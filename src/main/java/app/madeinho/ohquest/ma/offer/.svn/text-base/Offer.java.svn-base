package app.madeinho.ohquest.ma.offer;

import java.io.Serializable;
import java.util.Date;

import app.madeinho.ohquest.common.domain.BaseDomain;

public class Offer extends BaseDomain implements Serializable{

	/**
	 * 퀘스트 보상 관리 도메인
	 */
	private static final long serialVersionUID = 10047439323L;
	  
	  private String idmaOffer       ; 
	  private String idmaQuest       ;
	  private String offerType       ;
	  private String iduiItem        ;
	  private String offerValue      ;
	  private String offerRegDate    ;
	  private String offerEndDate    ;
	  private String plusRate        ;
	  private String minusRate       ;
	  private String contCreateDate  ;
	  private String contUpdateDate  ;
	  private String contSystem      ;
	public String getIdmaOffer() {
		return idmaOffer;
	}
	public void setIdmaOffer(String idmaOffer) {
		this.idmaOffer = idmaOffer;
	}
	public String getIdmaQuest() {
		return idmaQuest;
	}
	public void setIdmaQuest(String idmaQuest) {
		this.idmaQuest = idmaQuest;
	}
	public String getOfferType() {
		return offerType;
	}
	public void setOfferType(String offerType) {
		this.offerType = offerType;
	}
	public String getIduiItem() {
		return iduiItem;
	}
	public void setIduiItem(String iduiItem) {
		this.iduiItem = iduiItem;
	}
	public String getOfferValue() {
		return offerValue;
	}
	public void setOfferValue(String offerValue) {
		this.offerValue = offerValue;
	}
	public String getOfferRegDate() {
		return offerRegDate;
	}
	public void setOfferRegDate(String offerRegDate) {
		this.offerRegDate = offerRegDate;
	}
	public String getOfferEndDate() {
		return offerEndDate;
	}
	public void setOfferEndDate(String offerEndDate) {
		this.offerEndDate = offerEndDate;
	}
	public String getPlusRate() {
		return plusRate;
	}
	public void setPlusRate(String plusRate) {
		this.plusRate = plusRate;
	}
	public String getMinusRate() {
		return minusRate;
	}
	public void setMinusRate(String minusRate) {
		this.minusRate = minusRate;
	}
	public String getContCreateDate() {
		return contCreateDate;
	}
	public void setContCreateDate(String contCreateDate) {
		this.contCreateDate = contCreateDate;
	}
	public String getContUpdateDate() {
		return contUpdateDate;
	}
	public void setContUpdateDate(String contUpdateDate) {
		this.contUpdateDate = contUpdateDate;
	}
	public String getContSystem() {
		return contSystem;
	}
	public void setContSystem(String contSystem) {
		this.contSystem = contSystem;
	}
	public static long getSerialversionuid() {
		return serialVersionUID;
	}
	@Override
	public String toString() {
		return "Offer [idmaOffer=" + idmaOffer + ", idmaQuest=" + idmaQuest
				+ ", offerType=" + offerType + ", iduiItem=" + iduiItem
				+ ", offerValue=" + offerValue + ", offerRegDate="
				+ offerRegDate + ", offerEndDate=" + offerEndDate
				+ ", plusRate=" + plusRate + ", minusRate=" + minusRate
				+ ", contCreateDate=" + contCreateDate + ", contUpdateDate="
				+ contUpdateDate + ", contSystem=" + contSystem + "]";
	}
	
	
	
}
