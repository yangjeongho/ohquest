/*customize*/

.ui-tabs { position: relative; padding:0; zoom: 1; } 
.ui-tabs .ui-tabs-nav { margin: 0; padding:0;}

.ui-tabs .ui-tabs-nav li{float:left;margin-right:0px;background:url(../images/btn/tab_bg_l_univision.gif) no-repeat;}
.ui-tabs .ui-tabs-nav li span{display:block;margin-left:2px;}
.ui-tabs .ui-tabs-nav li span a{color:#333;line-height:14px;text-decoration:none;display:block;padding:6px 19px 5px; background:url(../images/btn/tab_bg_r_univision.gif) no-repeat 100% 0;}
.ui-tabs .ui-tabs-nav li a:hover{color:#333;}

.ui-tabs .ui-tabs-nav li.ui-tabs-selected {float:left;margin-right:0px;background:url(../images/btn/tab_bg_l_on_univision.gif) no-repeat;}
.ui-tabs .ui-tabs-nav li.ui-tabs-selected span{display:block;margin-left:2px; margin-right:2px;}
.ui-tabs .ui-tabs-nav li.ui-tabs-selected span a{color:#01572b; line-height:14px; text-decoration:none; display:block; padding:6px 19px 5px; font-weight:bold; background:url(../images/btn/tab_bg_r_on_univision.gif) no-repeat 100% 0;}
.ui-tabs .ui-tabs-nav li.ui-tabs-selected a:hover{color:#01572b;}
.ui-tabs .ui-tabs-panel { display: block; border:1px solid #ccc; background:#FFF;color:#333;clear:both; padding:5px; border-top:none;}
.ui-tabs .ui-tabs-hide { display: none !important; }
