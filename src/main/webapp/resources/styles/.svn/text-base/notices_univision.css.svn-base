@import "btn.css";
@import "etc.css";
@import "table_univision.css";
@import "layout.css";

html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, font, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td { margin:0; outline:0; border:0; padding:0; vertical-align:baseline;  }
ol, ul { list-style:none; }
blockquote, q { quotes:none; }
hr { border-bottom:1px solid #e1e1e1; border-top:1px solid #fff; border-left:1px solid #fff; border-right:1px solid #fff; outline:none !important;}
:active, :focus { outline:0; }
a:link, a:visited { text-decoration:none; color:#778690; }
a img { border:0; padding:0;}
ins { text-decoration:none; }
del { text-decoration:line-through; }
table { width:100%; margin:0 -1px; border-collapse:collapse; border-spacing:0; table-layout:fixed; }
caption { visibility:hidden; height:1px; }
.blind { position:absolute; visibility:hidden; z-index:-1; }
.clearfix:after { content:""; display:block; clear:both; }
.noline { background:none !important;}

html, body{ font-family:Arial; font-size:12px; margin:0; padding:0;  background:#f7f7f7; line-height:0; width:100%; height:100%; overflow-x:hidden; }


div section hgroup { background:url(../images/common/topbar_bar_bg.gif) 0 0 repeat-x; overflow:hidden;  padding:4px 15px 3px 10px; }
div section hgroup h6 {  float:left; font-size:12px; margin:0; padding:0; padding-top:5px; color:#fff; font-weight:bold; }
div section hgroup .util { float:right; }
div section hgroup .util ul li { float:left; }

#warp { width:97%; line-height:18px; padding:10px 18px;}
#title { width:100%; height:50px; line-height:18px; padding:0px 0px; } 
/* MAIN */
.main_box { 
background:url(../images/common/main_th_bg.gif) 0 0 repeat-x;
float:left;
border:solid 1px #d6dbe7;
margin:0 15px 15px 0;
width:100%;
height:258px;
padding:3px;
border-radius: 5px; 
-moz-border-radius: 5px; 
-webkit-border-radius: 5px;
overflow:hidden;
}
.main_box h1 { color:#000; font-weight:bold; font-size:12px; margin:0; padding:0 0 0 5px; float:left; background:none; }
.main_box .h1_btn { float:left; margin-left:5px; margin-top:-2px;}
.main_box .con { clear:both; padding:8px 5px; }

.main_txt_box ul { clear:both; margin-top:10px; }
.main_txt_box ul li.hd { background:url('../images/common/bl_01.gif') 0 3px no-repeat; color:#8c9ec6; padding:0 0 0 10px; margin-top:10px; font-weight:bold;}
.main_txt_box ul li { color:#666; padding:0 0 0 5px; }
.check_box { width:1000px; text-align:right; margin:20px 10px 0 0; color:#999; }

/* main TAB */
ul.tab01 { overflow:hidden; margin-bottom:5px;  background:url('../images/common/tab_01_line.gif') 0 0 repeat-x;  }
ul.tab01 li { float:left; overflow:hidden;   }
ul.tab01 li a { display:block; background:url('../images/common/tab_01_off.gif') left 0; float:left; color:#333; padding-left:23px; margin-left:2px; text-decoration:none; height:18px; cursor:pointer;  }
ul.tab01 li a span{ display:block; background:url('../images/common/tab_01_off.gif') right 0; float:left; color:#333; line-height:18px; padding-right:23px; padding-top:2px; text-decoration:none; height:18px; cursor:pointer; }

ul.tab01 li.select a { display:block; background:url('../images/common/tab_01_on.gif') left 0; float:left; color:#314d9c; padding-left:23px;  text-decoration:none; height:18px; cursor:pointer;  }
ul.tab01 li.select a span{ display:block; background:url('../images/common/tab_01_on.gif') right 0; float:left; color:#314d9c; font-weight:bold; line-height:18px; padding-right:23px; padding-top:2px; text-decoration:none; height:18px; cursor:pointer; }

/* main end */

/* SUB*/

.sub_left_box_01 { width:1030px; float:left; }
.serch_box_01 { background:#efefef; border:solid 1px #c6c7c6; width:100%; padding:2px 5px 2px 5px; color:#94657b; font-weight:bold; overflow:hidden;  }
.serch_box_01 img { vertical-align:middle }

h1 { background:url('../images/common/bl_02.gif') 0 2px no-repeat; margin:0; padding:0; font-size:12px; margin-top:10px; padding-left:13px; float:left; }
.txt_box_01 { clear:both; background:#efebde; border:solid 1px #cec794; padding:5px; width:100%; }
.txt_box_01 input.box01 { height:16px; border:solid 1px #e7dbce; }
.txt_box_01 td { padding:2px 0; }
.txt_box_01 td a { text-decoration:underline; font-weight:bold; }

.scroll_box_01 { width:1040px; height:110px; overflow-y:hidden; padding:1px;}
.scroll_box_02 { width:210px; height:144px; overflow:scroll; padding:1px;}
.scroll_box_03 { width:754px; height:155px; overflow:scroll; padding:1px;}

.sub_right_box_01 { 
margin-left:20px;
width:104px; 
float:left;
padding:5px 10px 5px 10px;
border-radius: 4px; 
-moz-border-radius: 4px; 
-webkit-border-radius: 4px; 
border:solid 1px #b5c3ce;
background:#f7f3f7;
}
.sub_right_box_01 ul li { background:url('../images/common/bl_04.gif') 0 10px no-repeat; padding-left:10px; line-height:26px; font-size:11px; border-bottom:dotted 1px #b5b2b5; }

.tab_box_01 { clear:both;  padding-top:10px; overflow:hidden;  }
.tab_box_01 ul li{ 
float:left; padding:2px 10px 2px 10px;
}
.tab_box_01 ul li span { font-size:14px; font-weight:bold; }
.tab_box_01 ul li.right{ 
float:right; margin-right:2px; padding:2px 10px 2px 10px;
}

.tab_box_01 ul li.off{ 
margin-left:2px;
float:left;
padding:2px 10px 2px 10px; 
cursor:pointer; 
border:solid 2px #bdbabd; 
color:#666; 
background:#f7f7f7;
border-bottom:solid 1px #849ace; 
text-align:center; 
border-radius: 4px 4px 0 0; 
-moz-border-radius: 4px 4px 0 0; 
-webkit-border-radius: 4px 4px 0 0; 
}

.tab_box_01 ul li.on{ 
margin-left:2px;
float:left;
padding:2px 10px 2px 10px; 
cursor:pointer; 
border:solid 2px #4a86c6; 
color:#315db5; 
background:#f7f7f7;
border-bottom:solid 2px #f7f7f7; 
font-weight:bold; 
text-align:center; 
border-radius: 4px 4px 0 0; 
-moz-border-radius: 4px 4px 0 0; 
-webkit-border-radius: 4px 4px 0 0; 
}

.tab_box_01_box { padding:5px; border:solid 1px #dedfde; border-top:solid 2px #849ace; margin-top:-2px; overflow:hidden; }
.tab_box_01_box ul li {float:left; margin-right:10px; }

h2 { background:url('../images/common/bl_03.gif') 0 5px no-repeat; margin:0; padding:0; font-size:11px; margin-top:10px; padding-left:13px; float:left; }

.tab_box_02_box { margin-top:5px;padding:5px; background:#dee7ef; border:solid 1px #dedfde; overflow:hidden; }
.tab_box_02_box ul li {float:left; margin-right:5px; }
.tab_02 {  overflow:hidden; font-size:11px; }
.tab_02 ul li.off{ 
margin-left:1px;
margin-right:0; 
float:left;
padding:3px 7px 2px 7px; 
cursor:pointer; 
color:#666;  
text-align:center; 
background:url('../images/common/tab_line.gif') right 6px no-repeat;
}
.tab_02 ul li.on{ 
margin-left:2px;
margin-right:0; 
float:left;
padding:2px 5px 2px 5px; 
cursor:pointer; 
border:solid 1px #a5b6c6; 
color:#316dad; 
background:#f7f7f7;
border-bottom:solid 1px #f7f7f7; 
font-weight:bold; 
text-align:center; 
border-radius: 4px 4px 0 0; 
-moz-border-radius: 4px 4px 0 0; 
-webkit-border-radius: 4px 4px 0 0; 
}

.tab_box_03_box {padding:2px;  border:solid 1px #a5b6c6; overflow:hidden; background:#f7f7f7; margin-top:-1px; }