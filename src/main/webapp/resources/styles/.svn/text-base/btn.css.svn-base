.cursorHand {cursor:pointer;_cursor:hand}

.btns1,
.btns1 *{ display:inline-block;overflow:visible;position:relative;margin:0;padding:0;border:0;background:url(../images/btn/btn_pack.gif) no-repeat;color:#d6e3ef; text-decoration:none !important;white-space:nowrap}
.btns1{margin-right:0px}
.btns1 *{left:0px;cursor:pointer;_cursor:hand}
.btns1.save,
.btns1.save *{height:21px;line-height:20px}
.btns1.save{background-position:left 0}
.btns1.save *{padding:1px 11px 0 11px;background-position:right 0;font-size:11px;}
.btns1 *:hover,
.btns1 *:active,
.btns1 *:focus{color:#fff}


