@CHARSET "UTF-8";

.tab_header{
	border:0px; font-family:Arial;
	font-size:11px;
	background:url(../images/btn/tab_bg_univision.gif) repeat-x 0 100%;
	margin-top:5px;
	float:left;position:relative; width:100%;
}

/*contents_box와 동일*/
.tab_body {
	background:#ffffff none;/*tab 배경*/
	font-size:100%;
	border:0px;
}

/* tab을 다른 div나 css를 입힐 경우 버그가 발생되는걸로 추정.. tab-contents를 적용할 경우 정상 가동..*/
.tab-contents { width:100%; overflow:auto;} 


